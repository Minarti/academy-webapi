{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "loopback-component-passport/lib/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
