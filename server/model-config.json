      "emailVerificationRequired": true
    }
  },
  "AccessToken": {
    "dataSource": "mongodb-ds",
    "public": false
  },
  "ACL": {
    "dataSource": "mongodb-ds",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongodb-ds",
    "public": false
  },
  "Role": {
    "dataSource": "mongodb-ds",
    "public": false
  },
  "UserCredential": {
    "dataSource": "mongodb-ds",
    "public": false
  },
  "UserIdentity": {
    "dataSource": "mongodb-ds",
    "public": false
  },
  "BaseModel": {
    "dataSource": null,
    "public": false
  },
  "Project": {
    "dataSource": "mongodb-ds",
    "public": true
  },
  "Backlog": {
    "dataSource": "mongodb-ds",
    "public": true
  },
  "Task": {
    "dataSource": "mongodb-ds",
    "public": true
  },
  "Account": {
    "dataSource": "mongodb-ds",
